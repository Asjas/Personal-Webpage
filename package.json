{
  "name": "personal_portfolio",
  "description": "This is my Personal Portfolio built with Nextjs.",
  "private": true,
  "version": "3.0.0",
  "author": "A-J Roos <asjas@outlook.com>",
  "keywords": [
    "nextjs"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "storybook": "npm-run-all --parallel test:generate-output start:storybook"
  },
  "dependencies": {
    "formik": "^1.3.2",
    "gatsby": "^2.0.56",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.10",
    "gatsby-plugin-netlify": "^2.0.5",
    "gatsby-plugin-netlify-cache": "^1.0.0",
    "gatsby-plugin-nprogress": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.13",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-plugin-typography": "^2.2.1",
    "gatsby-source-filesystem": "^2.0.9",
    "gatsby-transformer-sharp": "^2.1.8",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-error-overlay": "^5.1.0",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.0",
    "react-typography": "^0.16.13",
    "styled-components": "^4.1.1",
    "typography": "^0.16.17"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^4.0.9",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-centered": "^4.0.8",
    "@storybook/addon-graphql": "^4.0.8",
    "@storybook/addon-info": "^4.0.9",
    "@storybook/addon-jest": "^4.0.9",
    "@storybook/addon-knobs": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-storyshots": "^4.0.9",
    "@storybook/addon-storysource": "^4.0.9",
    "@storybook/addon-viewport": "^4.0.9",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "jest-styled-components": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.2",
    "react-testing-library": "^5.3.1",
    "snyk": "^1.111.0",
    "storybook-addon-jsx": "^6.0.0",
    "storybook-styled-components": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}

    "chalk": "^2.4.2",
    "formik": "^1.5.1",
    "gatsby": "^2.2.8",
    "gatsby-image": "^2.0.34",
    "gatsby-mdx": "^0.4.4",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-fullstory": "^2.0.4",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-robots-txt": "^1.4.0",

{
  "name": "asjas-personal-website",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run css:build && cross-env NODE_ENV=production remix build",
    "start": "cross-env NODE_ENV=development concurrently \"npm run css:watch\" \"remix run\" \"nodemon --ignore app start.js\"",
    "css:watch": "postcss styles --base styles --dir app/ -w",
    "css:build": "postcss styles --base styles --dir app/ --env production"
  },
  "dependencies": {
    "@remix-run/cli": "0.6.2",
    "@remix-run/express": "0.6.2",
    "@remix-run/loader": "0.6.2",
    "@remix-run/react": "0.6.2",
    "@tailwindcss/typography": "0.2.0",
    "@tryghost/content-api": "1.4.10",
    "@tryghost/helpers": "1.1.34",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "make-promises-safe": "5.1.0",
    "morgan": "1.10.0",
    "postcss-cli": "8.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "tailwindcss": "1.9.6"
  },
  "devDependencies": {
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "nodemon": "2.0.6"
  }
}

{
  "name": "personal_portfolio",
  "description": "This is my Personal Portfolio built with Hugo",
  "private": true,
  "version": "0.0.0",
  "author": "A-J Roos <asjas@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "hugo --configDir config",
    "dev": "hugo -DF server",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@sentry/browser": "5.15.5",
    "cypress": "4.5.0",
    "snyk": "1.299.0",
    "start-server-and-test": "1.10.10"
  },
  "devDependencies": {
    "@testing-library/cypress": "6.0.0",
    "husky": "4.2.3",
    "lint-staged": "10.2.3",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(toml|json|yml|yaml|css|scss|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "snyk": true
}

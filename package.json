{
  "name": "personal_portfolio",
  "description": "This is my Personal Portfolio built with Gatsby.js",
  "private": true,
  "version": "3.0.0",
  "author": "A-J Roos <asjas@outlook.com>",
  "keywords": [
    "react, gatsby, personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "storybook": "npm-run-all --parallel prebuild:storybook start:storybook"
  },
  "dependencies": {
    "@sentry/browser": "^4.4.2",
    "formik": "^1.4.2",
    "gatsby": "^2.0.85",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cache": "^1.0.0",
    "gatsby-plugin-nprogress": "^2.0.7",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-sitemap": "^2.0.4",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typography": "^2.2.5",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-source-prismic": "^2.2.0",
    "gatsby-transformer-sharp": "^2.1.10",
    "logrocket": "^0.6.19",
    "logrocket-react": "^3.0.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.2",
    "react-typography": "^0.16.18",
    "styled-components": "^4.1.3",
    "typography": "^0.16.18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-a11y": "^4.1.4",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-centered": "^4.1.4",
    "@storybook/addon-graphql": "^4.1.4",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/addon-jest": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-storyshots": "^4.1.4",
    "@storybook/addon-storysource": "^4.1.4",
    "@storybook/addon-viewport": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.6",
    "eslint": "5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-puppeteer": "^3.7.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-testing-library": "^5.4.2",
    "snyk": "^1.119.0",
    "storybook-addon-jsx": "^6.0.0",
    "storybook-styled-components": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}

    "@tailwindcss/typography": "0.4.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "autoprefixer": "10.2.4",
    "babel-eslint": "10.1.0",
    "concurrently": "6.0.0",
    "cssnano": "4.1.10",

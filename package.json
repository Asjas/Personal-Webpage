{
  "name": "personal_portfolio",
  "description": "This is my Personal Portfolio built with Hugo",
  "private": true,
  "version": "0.0.0",
  "author": "A-J Roos <asjas@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "hugo --configDir config",
    "dev": "hugo -DF server",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "lint:js": "eslint .",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@sentry/browser": "5.10.2",
    "cypress": "3.8.1",
    "date-fns": "2.8.1",
    "logrocket": "1.0.5",
    "snyk": "1.262.0",
    "start-server-and-test": "1.10.10"
  },
  "devDependencies": {
    "@testing-library/cypress": "5.0.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  },
  "snyk": true
}

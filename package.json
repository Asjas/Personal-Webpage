{
  "name": "personal_portfolio",
  "description": "This is my Personal Portfolio built with Nextjs.",
  "private": true,
  "version": "3.0.0",
  "author": "A-J Roos <asjas@outlook.com>",
  "keywords": [
    "nextjs"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next -p 3000",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "storybook": "npm-run-all --parallel test:generate-output start:storybook",
    "precommit": "npm run lint",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "ajv": "^6.5.4",
    "downshift": "^3.1.2",
    "formik": "^1.3.1",
    "graphql": "^0.13.2",
    "next": "^7.0.2",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-burger-menu": "^2.5.4",
    "react-dom": "^16.5.2",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@storybook/addon-a11y": "^4.0.0-alpha.25",
    "@storybook/addon-actions": "^4.0.0-rc.1",
    "@storybook/addon-centered": "^4.0.0-alpha.25",
    "@storybook/addon-graphql": "^4.0.0-alpha.25",
    "@storybook/addon-info": "^4.0.0-alpha.25",
    "@storybook/addon-jest": "^4.0.0-alpha.25",
    "@storybook/addon-knobs": "^4.0.0-alpha.25",
    "@storybook/addon-links": "^4.0.0-alpha.25",
    "@storybook/addon-storyshots": "^4.0.0-alpha.25",
    "@storybook/addon-storysource": "^4.0.0-alpha.25",
    "@storybook/addon-viewport": "^4.0.0-alpha.25",
    "@storybook/addons": "^4.0.0-alpha.25",
    "@storybook/react": "^4.0.0-alpha.25",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "jest-styled-components": "^6.2.2",
